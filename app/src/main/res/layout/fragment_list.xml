<?xml version="1.0" encoding="utf-8"?>
<com.sorrowdrug.gankiobyso.widget.StatusViewLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/statusView"
    tools:context=".ui.home.HomeItemFragment">

    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swiprefreshView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </android.support.v4.widget.SwipeRefreshLayout>
</com.sorrowdrug.gankiobyso.widget.StatusViewLayout>